{
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist/frontend"
      }
    },
    {
      "src": "backend/shortener-url.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/$1"
    },
    {
      "src": "/api(.*)",
      "dest": "/backend/shortener-url.js$1"
    }
  ]
}
