{
  "projects": [
    {
      "src": "frontend/",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/frontend"
      }
    },
    {
      "src": "backend/",
      "use": "@vercel/node"
    }
  ],
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "backend/shortener-url.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/shortener-url.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/$1"
    }
  ]
}
